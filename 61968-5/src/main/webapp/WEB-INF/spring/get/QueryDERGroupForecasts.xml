<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:jaxws="http://cxf.apache.org/jaxws"
	xmlns:cxf="http://cxf.apache.org/core"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd
	http://cxf.apache.org/core
	http://cxf.apache.org/schemas/core.xsd
	http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd">

	<import resource="classpath:META-INF/cxf/cxf.xml" />
	<import resource="classpath:META-INF/cxf/cxf-extension-soap.xml" />
	<import resource="classpath:META-INF/cxf/cxf-servlet.xml" />
	<import resource="../DateTimeBean.xml" />


	<bean id="get_queryDERGroupForecasts_header" name="get_queryDERGroupForecasts_header"
		class="com.sixthc.part5.get.QueryDERGroupForecasts.HeaderType">
		<property name="verb" value="change" />
		<property name="noun" value="DERGroups" />
		<property name="MessageID"
			value="#{ T(java.util.UUID).randomUUID().toString() }" />

	</bean>

	<!-- This should be part of get_queryDERGroups_Reply, when I figure out 
		how to do an array -->
	<bean id="get_queryDERGroupForecasts_error" name="get_queryDERGroupForecasts_error"
		class="com.sixthc.part5.get.QueryDERGroupForecasts.ErrorType">
		<property name="code" value="0.0" />
	</bean>

	<bean id="get_queryDERGroupForecasts_reply" name="get_queryDERGroupForecasts_reply"
		class="com.sixthc.part5.get.QueryDERGroupForecasts.ReplyType">
		<property name="result" value="OK" />
	</bean>

	<bean id="getQueryDERGroupForecastsBean" class="com.sixthc.server.ws.get.QueryDERGroupForecasts" />

	<jaxws:endpoint id="get_queryDERGroupForecastsBean"
		implementor="#getQueryDERGroupForecastsBean" address="/get/queryDERGroupForecasts">
		<jaxws:inInterceptors>
			<bean class="com.sixthc.interceptor.CIMLoggingInInterceptor">
				<property name="packageName" value="CIM DER" />
				<property name="packageGroupName" value="CIM DER Control" />
				<property name="messageName" value="CIM_DER(getQueryDERGroupForecasts)" />
				<property name="resultCode" value="success" />
				<property name="stage" value="serviceIn" />
			</bean>
		</jaxws:inInterceptors>
		<jaxws:outInterceptors>
					<bean class="com.sixthc.interceptor.ReplaceMessageInterceptor">
				<property name="messageName" value="CIM_DER(getQueryDERGroupForecasts)" />
				<property name="xsdFile"
					value="/resources/wsdl/get/xsd/DERGroupForecastQueriesMessage.xsd" />
				<property name="messageToValidate" value="DERGroupForecastQueriesResponseMessage" />
			</bean>
			<bean class="com.sixthc.interceptor.CIMLoggingOutInterceptor">
				<property name="packageName" value="CIM DER" />
				<property name="packageGroupName" value="CIM DER Control" />
				<property name="messageName" value="CIM_DER(getQueryDERGroupForecasts)" />
				<property name="resultCode" value="success" />
				<property name="stage" value="serviceOut" />
			</bean>

		</jaxws:outInterceptors>
		<jaxws:outFaultInterceptors>
			<bean class="com.sixthc.interceptor.CIMLoggingOutInterceptor">
				<property name="packageName" value="CIM DER" />
				<property name="packageGroupName" value="CIM DER Control" />
				<property name="messageName" value="CIM_DER(getQueryDERGroupForecasts)" />
				<property name="resultCode" value="fail" />
				<property name="stage" value="serviceOutFault" />
			</bean>
		</jaxws:outFaultInterceptors>
		<jaxws:inFaultInterceptors>
			<bean class="com.sixthc.interceptor.CIMLoggingInInterceptor">
				<property name="packageName" value="CIM DER" />
				<property name="packageGroupName" value="CIM DER Control" />
				<property name="messageName" value="CIM_DER(getQueryDERGroupForecasts)" />
				<property name="resultCode" value="fail" />
				<property name="stage" value="serviceInFault" />
			</bean>
		</jaxws:inFaultInterceptors>
		<jaxws:properties>
			<entry key="schema-validation-enabled" value="true" />
			<entry key="jaxb-validation-event-handler">
				<bean class="com.sixthc.interceptor.DetailValidationHandler" />
			</entry>
		</jaxws:properties>
	</jaxws:endpoint>


</beans>
