<xs:annotation/>
<xs:sequence>
<xs:element name="connectDisconnect" minOccurs="1" maxOccurs="1" type="xs:boolean" sawsdl:modelReference="http://iec.ch/TC57/CIM-generic#DERFunction.connectDisconnect">
