<div class="nav_main nav">
  <ul class="nav navbar-nav">
    <%= render 'layouts/navigation_links' %>
    <%= render 'layouts/nav_links_for_admin' %>
    <%= render 'layouts/nav_links_for_super' %>
  </ul>
  <ul class="nav navbar-nav navbar-right i_nav">
    <% if user_signed_in? %>
      <li><%if defined? current_user%><%= current_user.name%><% end %>
        <% if current_user.try(:admin?) %>
          <%= "(admin)" if defined? current_user && current_user.admin? %>
        <% end %>
        <% if current_user.try(:super?) %>
          <%= "(super)" if defined? current_user && current_user.admin? %>
        <% end %>
      </li>
    <% end %>
  </ul>
</div>