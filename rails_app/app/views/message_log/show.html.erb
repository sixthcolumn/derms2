<h1><%= @page_title %></h1>
<header class="header">
  <h1 class="header__heading" id="page-title"><%= content_for(:title) %></h1>
  <div class="header__actions">
    <%= link_to 'Back', :back, class: "button" %>
    <%= link_to 'Destroy', message_log_path(@log.message_log_id), class: "button",
              method: :delete,
              data: { confirm: 'Are you sure?' } %>
  </div>
</header>
<h3>Message</h3>
<div class="field-unit field-unit--string">
  <div class="field-unit__label">
    <label for="response_code">ID : </label>
  </div>
  <div class="field-unit__field">
    <%= @log.message_log_id %>
  </div>
</div>
<div class="field-unit field-unit--string">
  <div class="field-unit__label">
    <label for="response_code">Message Name : </label>
  </div>
  <div class="field-unit__field">
    <%= @log.message.name %>
  </div>
</div>
<div class="field-unit field-unit--string">
  <div class="field-unit__label">
    <label for="response_code">Create Date : </label>
  </div>
  <div class="field-unit__field">
    <%= @log.create_date %>
  </div>
</div>
<div class="field-unit field-unit--string">
  <div class="field-unit__label">
    <label for="response_code">Stage : </label>
  </div>
  <div class="field-unit__field">
    <%= @log.stage %>
  </div>
</div>
<div class="field-unit field-unit--string">
  <div class="field-unit__label">
    <label for="response_code">Result Code : </label>
  </div>
  <div class="field-unit__field">
    <%= @log.resultCode %>
  </div>
</div>
<div class="field-unit field-unit--string">
  <div class="field-unit__label">
    <label for="response_code">Message ID : </label>
  </div>
