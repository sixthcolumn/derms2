  </div>
</div>
<div class="field-unit field-unit--string">
  <div class="field-unit__label">
    <label for="response_code">Address : </label>
  </div>
  <div class="field-unit__field">
    <%= @log.address %>
  </div>
</div>
<div class="field-unit field-unit--string">
  <div class="field-unit__label">
    <label for="response_code">Content Type : </label>
  </div>
  <div class="field-unit__field">
